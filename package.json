{
  "name": "lottery-analysis-pro",
  "version": "1.0.0",
  "type": "module",
  "description": "A tool for advanced lottery number analysis and prediction.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint js/*.js",
    "build": "npm run build:workers && esbuild js/app.js --bundle --outfile=dist/bundle.js --sourcemap",
    "build:workers": "esbuild js/workers/ml-worker.js --bundle --outfile=dist/ml-worker.bundle.js && esbuild js/workers/backtest-worker.js --bundle --outfile=dist/backtest-worker.bundle.js && esbuild js/workers/optimization-worker.js --bundle --outfile=dist/optimization-worker.bundle.js",
    "pretest:e2e": "npm run build",
    "test:e2e": "playwright test",
    "posttest:e2e": "npx playwright show-report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikeBehar/lottery-analysis-pro.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mikeBehar/lottery-analysis-pro/issues"
  },
  "homepage": "https://github.com/mikeBehar/lottery-analysis-pro#readme",
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@playwright/test": "^1.55.0",
    "@tensorflow/tfjs": "^4.22.0",
    "esbuild": "^0.25.9",
    "eslint": "^9.33.0",
    "jest": "^30.1.1",
    "jest-environment-jsdom": "^30.1.1",
    "live-server": "^1.2.2"
  }
}
